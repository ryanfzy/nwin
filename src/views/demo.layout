<window name="demo">
	<grid>
		<grid.rowdefinition>
			<rowdefinition height="auto" />
			<rowdefinition height="5" />
			<rowdefinition height="auto" />
			<rowdefinition height="5" />
			<rowdefinition height="auto" />
			<rowdefinition height="5" />
			<rowdefinition height="auto" />
			<rowdefinition height="5" />
			<rowdefinition height="auto" />
			<rowdefinition height="5" />
			<rowdefinition height="auto" />
		</grid.rowdefinition>
		<grid.columndefinition>
			<columndefinition width="auto" />
			<columndefinition width="5" />
			<columndefinition width="auto" />
			<columndefinition width="5" />
			<columndefinition width="auto" />
			<columndefinition width="5" />
			<columndefinition width="auto" />
			<columndefinition width="5" />
			<columndefinition width="auto" />
		</grid.columndefinition>
		<border grid.row="0"
			grid.column="0"
			grid.colspan="9"
			width="270"
			height="50">
			<grid>
				<grid.rowdefinition>
					<rowdefinition height="auto" />
					<rowdefinition height="5" />
					<rowdefinition height="auto" />
				</grid.rowdefinition>
				<textblock grid.row="0"
					width="270"
					height="20"
					horizontalalignment="right"
					text="{binding Text1}" />
				<textblock grid.row="2"
					width="270"
					height="20"
					horizontalalignment="right"
					text="{binding Text2}" />
			</grid>
		</border>
		<button grid.row="2"
			grid.column="0"
			grid.colspan="9"
			content="clear"
			width="270"
			height="50"
			command="{binding ClearCommand}" />
		<button grid.row="4"
			grid.column="0"
			width="50"
			height="50"
			content="7"
			command="{binding SevenCommand}" />
		<button grid.row="4"
			grid.column="2"
			width="50"
			height="50"
			content="8"
			command="{binding EightCommand}" />
		<button grid.row="4"
			grid.column="4"
			width="50"
			height="50"
			content="9"
			command="{binding NineCommand}" />
		<button grid.row="4"
			grid.column="6"
			width="50"
			height="50"
			content="/"
			command="{binding DivideCommand}" />
		<button grid.row="4"
			grid.column="8"
			width="50"
			height="50"
			content="%" />
		<button grid.row="6"
			grid.column="0"
			width="50"
			height="50"
			content="4"
			command="{binding FourCommand}" />
		<button grid.row="6"
			grid.column="2"
			width="50"
			height="50"
			content="5"
			command="{binding FiveCommand}" />
		<button grid.row="6"
			grid.column="4"
			width="50"
			height="50"
			content="6"
			command="{binding SixCommand}" />
		<button grid.row="6"
			grid.column="6"
			width="50"
			height="50"
			content="*"
			command="{binding MultiCommand}" />
		<button grid.row="6"
			grid.column="8"
			width="50"
			height="50"
			content="1/X" />
		<button grid.row="8"
			grid.column="0"
			width="50"
			height="50"
			content="1"
			command="{binding OneCommand}" />
		<button grid.row="8"
			grid.column="2"
			width="50"
			height="50"
			content="2"
			command="{binding TwoCommand}" />
		<button grid.row="8"
			grid.column="4"
			width="50"
			height="50"
			content="3"
			command="{binding ThreeCommand}" />
		<button grid.row="8"
			grid.column="6"
			width="50"
			height="50"
			content="-"
			command="{binding MinusCommand}" />
		<button grid.row="8"
			grid.rowspan="3"
			grid.column="8"
			width="50"
			height="105"
			content="="
			command="{binding EqCommand}" />
		<button grid.row="10"
			grid.column="0"
			grid.colspan="3"
			width="105"
			height="50"
			content="0"
			command="{binding ZeroCommand}" />
		<button grid.row="10"
			grid.column="4"
			width="50"
			height="50"
			content="." />
		<button grid.row="10"
			grid.column="6"
			width="50"
			height="50"
			content="+"
			command="{binding PlusCommand}" />
	</grid>
</window>
